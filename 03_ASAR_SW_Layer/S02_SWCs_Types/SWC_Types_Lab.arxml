<?xml version="1.0" encoding="UTF-8"?>
<AUTOSAR xmlns="http://autosar.org/schema/r4.0" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://autosar.org/schema/r4.0 file//../../AUTOSAR_ARXML_Schema/AUTOSAR_00051.xsd">
	<AR-PACKAGES>
		<AR-PACKAGE>
			<!-- Emergency Automatic Breaking System Application Package -->
			<SHORT-NAME>SWCs_EABS_Package</SHORT-NAME>
			<ELEMENTS>
				<!-- Sensor Actuator SWC for Lidar Sensor -->
				<SENSOR-ACTUATOR-SW-COMPONENT-TYPE>
					<SHORT-NAME>LidarSensor_SAC_SWC1</SHORT-NAME>
				</SENSOR-ACTUATOR-SW-COMPONENT-TYPE>
				<!-- Sensor Actuator SWC for Speed Sensor -->
				<SENSOR-ACTUATOR-SW-COMPONENT-TYPE>
					<SHORT-NAME>SpeedSensor_SAC_SWC2</SHORT-NAME>
				</SENSOR-ACTUATOR-SW-COMPONENT-TYPE>
				<!-- ECU Abstraction SWC for I/O Hardware -->
				<ECU-ABSTRACTION-SW-COMPONENT-TYPE>
					<SHORT-NAME>IO_ECUAbstr_SWC3</SHORT-NAME>
				</ECU-ABSTRACTION-SW-COMPONENT-TYPE>
				<!-- Application SWC for the main algorithm of the EABS -->
				<APPLICATION-SW-COMPONENT-TYPE>
					<SHORT-NAME>EABS_App_SWC4</SHORT-NAME>
				</APPLICATION-SW-COMPONENT-TYPE>
				<!-- Parameter SWC for the different distance parameters -->
				<PARAMETER-SW-COMPONENT-TYPE>
					<SHORT-NAME>d_P_SWC5</SHORT-NAME>
				</PARAMETER-SW-COMPONENT-TYPE>
				<!-- Sensor Actuator SWC for the LED -->
				<SENSOR-ACTUATOR-SW-COMPONENT-TYPE>
					<SHORT-NAME>LEDWarning_SAC_SWC6</SHORT-NAME>
				</SENSOR-ACTUATOR-SW-COMPONENT-TYPE>
				<!-- CDD SWC for fast acess of the Brakes -->
				<COMPLEX-DEVICE-DRIVER-SW-COMPONENT-TYPE>
					<SHORT-NAME>Brake_CDD_SWC7</SHORT-NAME>
				</COMPLEX-DEVICE-DRIVER-SW-COMPONENT-TYPE>
				<!-- Service SWC for the Diagnostic Manager -->
				<SERVICE-SW-COMPONENT-TYPE>
					<SHORT-NAME>DiagM_Service_SWC8</SHORT-NAME>
				</SERVICE-SW-COMPONENT-TYPE>
				<!-- NV Block SWC for accessing the NVM -->
				<NV-BLOCK-SW-COMPONENT-TYPE>
					<SHORT-NAME>Flash_NVBLK_SWC9</SHORT-NAME>
				</NV-BLOCK-SW-COMPONENT-TYPE>
			</ELEMENTS>
		</AR-PACKAGE>
	</AR-PACKAGES>
</AUTOSAR>