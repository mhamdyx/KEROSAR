<?xml version="1.0" encoding="UTF-8"?>
<AUTOSAR xmlns="http://autosar.org/schema/r4.0" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://autosar.org/schema/r4.0 file//../../AUTOSAR_ARXML_Schema/AUTOSAR_00051.xsd">
	<AR-PACKAGES>
		<AR-PACKAGE>
			<!-- Learn in depth, Eng. Mahmoud Hamdy, Unit3: SW Layer, Session3: SR Interface -->
			<SHORT-NAME>SR_Interface_Lab</SHORT-NAME>
			<ELEMENTS>
				<!-- SR Interface with 3 data elements -->
				<SENDER-RECEIVER-INTERFACE>
					<SHORT-NAME>SRI_Lidar</SHORT-NAME>
					<DATA-ELEMENTS>
						<!-- Variable 1: uint16 Lidar_ObjectDistance = 7 -->
						<VARIABLE-DATA-PROTOTYPE>
							<SHORT-NAME>Lidar_ObjectDistance</SHORT-NAME>
							<TYPE-TREF DEST="IMPLEMENTATION-DATA-TYPE">/AUTOSAR_PlatformTypes/ImplementationDataTypes/uint16</TYPE-TREF>
							<INIT-VALUE>
								<NUMERICAL-VALUE-SPECIFICATION>
									<VALUE>7</VALUE>
								</NUMERICAL-VALUE-SPECIFICATION>
							</INIT-VALUE>
						</VARIABLE-DATA-PROTOTYPE>
						<!-- Variable 2: uint16 Lidar_ObjectID -->
						<VARIABLE-DATA-PROTOTYPE>
							<SHORT-NAME>Lidar_ObjetID</SHORT-NAME>
							<TYPE-TREF DEST="IMPLEMENTATION-DATA-TYPE">/AUTOSAR_PlatformTypes/ImplementationDataTypes/uint16</TYPE-TREF>
						</VARIABLE-DATA-PROTOTYPE>
						<!-- Variable 3: uint32 Lidar_ObjectAngle -->
						<VARIABLE-DATA-PROTOTYPE>
							<SHORT-NAME>Lidar_ObjectAngle</SHORT-NAME>
							<TYPE-TREF DEST="IMPLEMENTATION-DATA-TYPE">/AUTOSAR_PlatformTypes/ImplementationDataTypes/uint32</TYPE-TREF>
						</VARIABLE-DATA-PROTOTYPE>
					</DATA-ELEMENTS>
				</SENDER-RECEIVER-INTERFACE>
				<!-- Sensor Actuator SWC (to send Lidar Data to the main Algorithm) -->
				<SENSOR-ACTUATOR-SW-COMPONENT-TYPE>
					<SHORT-NAME>LidarSensor_SWC</SHORT-NAME>
					<PORTS>
						<P-PORT-PROTOTYPE>
							<SHORT-NAME>SR_PP_LidarData</SHORT-NAME>
							<PROVIDED-INTERFACE-TREF DEST="SENDER-RECEIVER-INTERFACE">/SR_Interface_Lab/SRI_Lidar</PROVIDED-INTERFACE-TREF>
						</P-PORT-PROTOTYPE>
					</PORTS>
				</SENSOR-ACTUATOR-SW-COMPONENT-TYPE>
				<!-- Application SWC (Main Algo that operates on the Lidar data) -->
				<APPLICATION-SW-COMPONENT-TYPE>
					<SHORT-NAME>LidarAlgo_SWC</SHORT-NAME>
					<PORTS>
						<R-PORT-PROTOTYPE>
							<SHORT-NAME>SR_RP_LidarData</SHORT-NAME>
							<REQUIRED-INTERFACE-TREF DEST="SENDER-RECEIVER-INTERFACE">/SR_Interface_Lab/SRI_Lidar</REQUIRED-INTERFACE-TREF>
						</R-PORT-PROTOTYPE>
					</PORTS>
				</APPLICATION-SW-COMPONENT-TYPE>
			</ELEMENTS>
		</AR-PACKAGE>
	</AR-PACKAGES>
</AUTOSAR>